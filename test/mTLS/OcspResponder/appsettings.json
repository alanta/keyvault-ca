{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "KeyVaultCa": "Debug"
    }
  },
  "AllowedHosts": "*",
  "OcspResponder": {
    "KeyVaultUrl": "https://YOURVAULT.vault.azure.net",
    "OcspSignerCertName": "ocsp-signer",
    "IssuerCertName": "root-ca",
    "ResponseValidityMinutes": 1440,
    "EnableCaching": true
  }
}
